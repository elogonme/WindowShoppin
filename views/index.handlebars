<nav class="navbar pb-0">
  <div class="container-fluid">
    <a class="navbar-brand" href="/">
      <img src="/images/Window.png" height="60" alt="Logo" class="logo">
    </a>
    <div class="navbar-header">
      <a class="navbar-brand btn btn-outline-dark" href="/logout">
        Logout
        <i class="material-icons fs-2">exit_to_app</i>
      </a>
    </div>
  </div>
</nav>


<div class="container-fluid">
  <div class="row">
    <div class="container text-center">
      <h2 id="subtitle">Welcome! Track your shoppin' favorites! <span id="member-name"></span></h2>
    </div>
  </div>
  <div class="container">
    <form id="add-item-form" style="display: flex">
      <div class="input-group mb-3">
        <input type="text" id="url-input" id="url-input" class="new-item form-control" placeholder="Enter an Item URL">
        <div class="input-group-append">
          <button class="btn btn-outline-primary" type="submit">
            <i class="material-icons fs-2">add_shopping_cart</i>
            Add Item</button>
        </div>
      </div>
    </form>
    <!-- Spinner while item is loading -->
    <div class="loader invisible" role="status"></div>
    <!-- this is where categories should show up -->
    <div class="col-md-12 panel" id="all-items">
      <div class="row justify-content-end mb-2">
        <div class="col">
          <h3 class="text-center text-primary">Your Saved Items</h4>
        </div>
        <button id="update-all" class="btn btn-outline-warning mr-3 text-dark">Check All</button>
      </div>
      
        {{#each items}}
        {{> items/item-block}}
        {{/each}}
    </div>
  </div>
</div>
</div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
<script src="/js/dashboard.js" type="text/javascript"></script>

